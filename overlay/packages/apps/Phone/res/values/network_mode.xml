<!--
    Network modes from:
    frameworks/base/telephony/java/com/android/internal/telephony/RILConstants.java

    0: GSM/WCDMA (WCDMA preferred)
    1: GSM only
    2: WCDMA only
    3: GSM/WCDMA (auto mode, according to PRL)
    4: CDMA and EvDo (auto mode, according to PRL)
    5: CDMA only
    6: EvDo only
    7: GSM/WCDMA, CDMA, and EvDo (auto mode, according to PRL)
    8: LTE, CDMA and EvDo
    9: LTE, GSM/WCDMA
    10: LTE, CDMA, EvDo, GSM/WCDMA
    11: LTE Only mode.
    12: LTE/WCDMA
-->

<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Mobile network mode choices for a global phone [CHAR LIMIT=25] -->
    <string-array name="preferred_network_mode_choices_lte">
        <item>LTE/WCDMA</item>
        <item>LTE Only</item>
        <item>LTE/CDMA/EvDo/GSM/WCDMA</item>
        <item>LTE/GSM/WCDMA</item>
        <item>LTE/CDMA/EvDo</item>
        <item>CDMA/EvDo/GSM/WCDMA</item>
        <item>CDMA/EvDo</item>
        <item>GSM/WCDMA</item>
    </string-array>
    <string-array name="preferred_network_mode_values_lte" translatable="false">
        <item>"12"</item>
        <item>"11"</item>
        <item>"10"</item>
        <item>"9"</item>
        <item>"8"</item>
        <item>"7"</item>
        <item>"4"</item>
        <item>"3"</item>
    </string-array>

    <!-- LTE/CDMA network mode to use for toggleLTE(true) -->
    <integer name="toggleLTE_lte_cdma_nt_mode">8</integer>
</resources>
