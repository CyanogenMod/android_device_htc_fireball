        <ctl name="RX4 DSM MUX" value="DSM_INV" />
        <ctl name="LINEOUT1 Volume" value="10" />
        <ctl name="LINEOUT3 Volume" value="10" />
        <ctl name="RX3 Digital Volume" value="97" /> <!-- Modified from HTC: 84 -->
        <ctl name="RX4 Digital Volume" value="97" /> <!-- Modified from HTC: 84 -->
        <ctl name="SPK AMP EN Switch" value="1" />
        <ctl name="FENS Enable" value="1" />
    </path>

    <!-- VOIP Speaker -->
    <path name="voip-speaker-comm">
        <path name="speaker" />
        <ctl name="FENS Enable" value="1" />
    </path>

    <!-- Input devices -->

    <!-- BT SCO Tx -->
    <path name="bt-sco-mic">
    </path>

    <!-- BT SCO WB Tx -->
    <path name="bt-sco-mic-wb">
    </path>

    <!-- Camcorder Tx -->
    <path name="camcorder-mic">
