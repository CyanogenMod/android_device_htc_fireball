    <ctl name="Voice_Tx Mixer SLIM_0_TX_Voice" value="0" />
    <ctl name="Voip_Tx Mixer AFE_PCM_TX_Voip" value="0" />
    <ctl name="Voip_Tx Mixer INTERNAL_BT_SCO_TX_Voip" value="0" />
    <ctl name="Voip_Tx Mixer SLIM_0_TX_Voip" value="0" />

    <!-- echo reference -->
    <ctl name="EC_REF_RX" value="NONE" />
    <!-- echo reference end -->

    <!-- These are audio route (FE to BE) specific mixer settings -->

    <!-- HiFi -->
    <path name="deep-buffer-playback">
        <ctl name="SLIMBUS_0_RX Audio Mixer MultiMedia1" value="1" />
    </path>

    <!-- HiFiPROXY Rx -->
    <path name="deep-buffer-playback afe-proxy">
        <ctl name="AFE_PCM_RX Audio Mixer MultiMedia1" value="1" />
    </path>

    <!-- HiFiBT SCO Rx -->
    <path name="deep-buffer-playback bt-sco">
        <ctl name="INTERNAL_BT_SCO_RX Audio Mixer MultiMedia1" value="1" />
    </path>

    <!-- HiFiBT SCO WB Rx -->
    <path name="deep-buffer-playback bt-sco-wb">
        <ctl name="INTERNAL_BT_SCO_RX Audio Mixer MultiMedia1" value="1" />
    </path>

    <!-- HiFi Lowlatency -->
    <path name="low-latency-playback">
        <ctl name="SLIMBUS_0_RX Audio Mixer MultiMedia5" value="1" />
    </path>

    <!-- HiFi LowlatencyBT SCO Rx -->
    <path name="low-latency-playback bt-sco">
        <ctl name="INTERNAL_BT_SCO_RX Audio Mixer MultiMedia5" value="1" />
    </path>

    <!-- HiFi LowlatencyBT SCO WB Rx -->
    <path name="low-latency-playback bt-sco-wb">
        <ctl name="INTERNAL_BT_SCO_RX Audio Mixer MultiMedia5" value="1" />
    </path>

    <!-- HiFi LowlatencyPROXY Rx -->
    <path name="low-latency-playback afe-proxy">
        <ctl name="AFE_PCM_RX Audio Mixer MultiMedia5" value="1" />
    </path>

    <!-- HiFi Rec -->
    <path name="audio-record">
        <ctl name="MultiMedia1 Mixer SLIM_0_TX" value="1" />
    </path>

    <!-- HiFi RecBT SCO Tx -->
    <path name="audio-record bt-sco">
        <ctl name="MultiMedia1 Mixer INTERNAL_BT_SCO_TX" value="1" />
    </path>

    <!-- HiFi RecBT SCO WB Tx -->
    <path name="audio-record bt-sco-wb">
        <ctl name="MultiMedia1 Mixer INTERNAL_BT_SCO_TX" value="1" />
    </path>

    <!-- HiFi Rec Compressed -->
    <path name="audio-record-compress">
        <ctl name="MultiMedia4 Mixer SLIM_0_TX" value="1" />
    </path>

    <!-- HiFi Lowlatency Rec -->
    <path name="low-latency-record">
        <ctl name="MultiMedia5 Mixer SLIMBUS_0_TX" value="1" />
    </path>

    <!-- HiFi Lowlatency RecBT SCO Tx -->
    <path name="low-latency-record bt-sco">
        <ctl name="MultiMedia5 Mixer INTERNAL_BT_SCO_TX" value="1" />
    </path>

    <!-- HiFi Lowlatency RecBT SCO WB Tx -->
    <path name="low-latency-record bt-sco-wb">
        <ctl name="MultiMedia5 Mixer INTERNAL_BT_SCO_TX" value="1" />
    </path>

    <!-- Voice Call -->
    <path name="voice-call">
        <ctl name="SLIM_0_RX_Voice Mixer CSVoice" value="1" />
        <ctl name="Voice_Tx Mixer SLIM_0_TX_Voice" value="1" />
    </path>

    <!-- Voice Call Base BT SCO Tx -->
    <path name="voice-call-bt-sco-tx">
        <ctl name="INTERNAL_BT_SCO_RX_Voice Mixer CSVoice" value="1" />
        <ctl name="Voice_Tx Mixer INTERNAL_BT_SCO_TX_Voice" value="1" />
    </path>

    <!-- Voice CallBT SCO Tx -->
    <path name="voice-call bt-sco">
        <ctl name="Internal BTSCO SampleRate" value="8000" />
        <path name="voice-call-bt-sco-tx" />
    </path>

    <!-- Voice CallBT SCO WB Tx -->
    <path name="voice-call bt-sco-wb">
        <ctl name="Internal BTSCO SampleRate" value="16000" />
        <path name="voice-call-bt-sco-tx" />
    </path>

    <!-- Voice Call IP -->
    <path name="voip-call">
        <ctl name="SLIM_0_RX_Voice Mixer Voip" value="1" />
        <ctl name="Voip_Tx Mixer SLIM_0_TX_Voip" value="1" />
    </path>

    <!-- Voice Call IP Base BT SCO Tx -->
    <path name="voip-call-bt-sco-tx">
        <ctl name="INTERNAL_BT_SCO_RX_Voice Mixer Voip" value="1" />
        <ctl name="Voip_Tx Mixer INTERNAL_BT_SCO_TX_Voip" value="1" />
    </path>

    <!-- Voice Call IPBT SCO Tx -->
    <path name="voip-call bt-sco">
        <ctl name="Internal BTSCO SampleRate" value="8000" />
        <path name="voip-call-bt-sco-tx" />
    </path>

    <!-- Voice Call IPBT SCO WB Tx -->
    <path name="voip-call bt-sco-wb">
        <ctl name="Internal BTSCO SampleRate" value="16000" />
        <path name="voip-call-bt-sco-tx" />
    </path>

    <!-- (Not provided by HTC) -->
    <path name="incall-rec-uplink">
        <ctl name="MultiMedia1 Mixer VOC_REC_UL" value="1" />
    </path>

    <!-- (Not provided by HTC) -->
    <path name="incall-rec-downlink">
        <ctl name="MultiMedia1 Mixer VOC_REC_DL" value="1" />
    </path>

    <!-- (Not provided by HTC) -->
    <path name="incall-rec-uplink-and-downlink">
        <path name="incall-rec-uplink" />
        <path name="incall-rec-downlink" />
    </path>

    <!-- HiFi RecPROXY Tx -->
    <path name="afe-proxy-record">
        <ctl name="MultiMedia1 Mixer AFE_PCM_TX" value="1" />
    </path>

    <!-- These are actual sound device specific mixer settings -->

    <!-- Output devices -->

    <!-- BT SCO Rx -->
    <path name="bt-sco-headset">
    </path>

    <!-- BT SCO WB Rx -->
    <path name="bt-sco-headset-wb">
    </path>

    <!-- Earpiece -->
    <path name="handset">
        <ctl name="SLIM_0_RX Channels" value="One" />
        <ctl name="RX1 MIX1 INP1" value="RX1" />
        <ctl name="DAC1 Switch" value="1" />
        <ctl name="RX1 Digital Volume" value="84" />
        <ctl name="EAR PA Gain" value="POS_6_DB" />
    </path>

    <!-- Headphones -->
    <path name="headphones">
        <ctl name="SLIM_0_RX Channels" value="Two" />
        <ctl name="RX1 MIX1 INP1" value="RX1" />
        <ctl name="RX2 MIX1 INP1" value="RX2" />
        <ctl name="HPHL DAC Switch" value="1" />
        <ctl name="RX1 Digital Volume" value="84" />
        <ctl name="RX2 Digital Volume" value="84" />
        <ctl name="HPHL Volume" value="8" />
        <ctl name="HPHR Volume" value="8" />
    </path>

    <!-- PROXY Rx -->
    <path name="afe-proxy">
    </path>

